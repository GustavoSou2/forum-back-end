{
  "name": "blog_backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:server": "ts-node-dev --transpile-only src/shared/infra/http/server.ts",
    "typeorm": "ts-node-dev node_modules/typeorm/cli.js",
    "typeorm:run": "ts-node-dev node_modules/typeorm/cli.js -d ormconfig.ts migration:run",
    "build": "rimraf dist/* && tsc -p tsconfig.json && tscpaths -p tsconfig.json -s . -o ./dist",
    "start": "node dist/src/shared/infra/http/server.js",
    "deploy": "git add . && git commit -m Heroku && git push"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/dotenv-flow": "^3.2.0",
    "@types/express": "^4.17.14",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/uuid": "^8.3.4",
    "nodemon": "^2.0.20",
    "ts-node-dev": "^2.0.0",
    "tscpaths": "^0.0.9",
    "ts-node": "^10.7.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "bcrypt": "^5.1.0",
    "celebrate": "^15.0.1",
    "dotenv": "^16.0.3",
    "dotenv-flow": "^3.2.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mysql": "npm:mysql2@^2.1.0",
    "tsyringe": "^4.7.0",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.10"
  }
}
